  <script>
    function isValid(element, evnt) {
      var charC = evnt.which ? evnt.which : evnt.keyCode;
      if (charC > 31 && (charC < 48 || charC > 57)) return false;
      return true;
    }

    function CheckPhone(thisEl, evnt) {
      var result = isValid(thisEl, evnt);
      return result;
    }
  </script>

