  <form id='student' method='post' action='/index'>
    <div class="orig">
      <div class="last">
        <a class="form-pic-box"><img src="./assets/Images/Form Image.jpg" class="form-pic" /></a>
      </div>
      <div class="encompass">
        <div class="surround">
          <h2>
            Register with Envision now!
          </h2>
        </div>
        <div class="two surround">
          <input class="form-control" autocomplete="off" required type="text" id="name1" name='fname'
            placeholder="First Name" />
          <input class="form-control" autocomplete="off" required type="text" id="name2" name='lname'
            placeholder="Last Name" />
        </div>
        <div>
          <div class="surround four">
            <input class="form-control" autocomplete="off" required type="email" id="email" name='email'
              placeholder="Email" />
          </div>
        </div>
        <div>
          <div class="surround five">
            <select class="form-control" id="country-code" name="country-code">
              <option value="india">+91</option>
            </select>
            <input class="form-control" autocomplete="off" required type="tel" id="phone" name='phone'
              pattern="[0-9]{10}" maxlength="10" onkeypress="return CheckPhone(this, event);"
              placeholder="Phone Number" />
          </div>
        </div>
        <div class="surround">
          <div class="six">
            <select class="form-control" id="nearest-office" name="nearest-office">
